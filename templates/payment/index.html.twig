{% extends 'base.html.twig' %}

{% block title %}Payment{% endblock %}

{% block main %}
    <div class="container body-content span=8 offset=2">
        <div id="notifications-table">
            <div id="loadingBox" class="notification">
                <span>Зареждане на данните …</span>
            </div>
            <div id="errorBox" class="notification">
                <span>Грешка в данните!</span>
            </div>
        </div>
        <div class="table-responsive">
            <legend>Всички плащания</legend>
            <table id="dataTable"  class="display" cellspacing="0" width="100%"></table>
        </div>
    </div>
{% endblock %}

{% block jsCustomPage %}
    {{ encore_entry_script_tags('pagePaymentAjax') }}
{% endblock %}
